package practice.calculator;

public class Calculator {
	
	// 고정된 수
	public void plus() {
		System.out.println("15 + 4 = " + (15 + 4));	
	}
	
	public void minus() {
		System.out.println("15 - 4 = " + (15 - 4));			
	}
	
	public void multiple() {
		System.out.println("15 * 4 = " + (15 * 4));			
	}
	
	public void division() {
		System.out.println("15 / 4 = " + (15 / 4));			
	}

	
	// 매개변수로 입력 받아오기
	public void addition(int firstNum, int secondNum) {
		System.out.println();
	}
}
