package practice.calculator;

public class Calculator {
	
	public void plus() {
		System.out.println("15 + 4 = " + (15 + 4));	
	}
	
	public void minus() {
		System.out.println("15 + 4 = " + (15 + 4));			
	}
	
	public void multiple() {
		System.out.println("15 + 4 = " + (15 + 4));			
	}
	
	public void division() {
		System.out.println("15 + 4 = " + (15 + 4));			
	}

}
