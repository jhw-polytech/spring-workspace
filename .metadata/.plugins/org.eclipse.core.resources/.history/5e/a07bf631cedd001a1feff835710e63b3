<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 					 http://www.springframework.org/schema/beans/spring-beans.xsd ">

<!-- setter 형태로 쓸 것이다! --> 				
	<bean class="di.collection.ListAddress" id="list" >
		<property name="address" >
			<list> <!-- 리스트형은 <list> 태그를 써서 나타낸다.  -->
				<value>서울시 서초구</value> <!-- 리스트형 안에 들어가는 문자열은 <value> 태그로 나타낸다. -->
				<value>경기도 광명시 철산동</value>
				<value>부산광역시 동구</value>
			</list>
		</property>	
	</bean>	 					 
	<bean class="di.collection.ListAddress" id="list2" >
		<property name="address2" >
			<list value-type="jav.util.String"> <!-- value에 들어갈 type을 정의  -->
				<value>서울시 서초구</value>
				<value>경기도 광명시 철산동</value>
				<value>부산광역시 동구</value>
			</list>
		</property>	
	</bean>	 					 
</beans>